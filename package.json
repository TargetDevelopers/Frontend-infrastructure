{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier-eslint --write $PWD/'**/*.{ts,tsx}'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "storybook": "start-storybook -s ./public -p 9009",
    "build-storybook": "build-storybook",
    "test": "jest",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@badrap/bar-of-progress": "^0.2.2",
    "@next/bundle-analyzer": "^14.0.3",
    "antd": "^5.12.1",
    "axios": "^1.6.2",
    "js-cookie": "^3.0.5",
    "mapbox-gl": "^3.0.0",
    "moment": "^2.29.4",
    "next": "^14.0.3",
    "next-cookies": "^2.0.3",
    "next-redux-wrapper": "^8.1.0",
    "next-translate": "^2.6.2",
    "next.js": "^1.0.3",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-customizable-progressbar": "^1.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.2",
    "redux": "^5.0.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.2.3",
    "redux-thunk": "^3.1.0",
    "styled-components": "^6.1.1",
    "swr": "^2.2.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.6.3",
    "@storybook/addon-controls": "^7.6.3",
    "@storybook/addon-docs": "^7.6.3",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-viewport": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "prettier-eslint-cli": "^8.0.1",
    "react-test-renderer": "^18.2.0",
    "redux-mock-store": "^1.5.4",
    "typescript": "^5.3.3"
